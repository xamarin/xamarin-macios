			AppBundleDir="$(AppBundleDir)"
			ProvisioningProfile="$(_ProvisioningProfile)"
			TargetFrameworkMoniker="$(_ComputedTargetFrameworkMoniker)"
			EmbeddedProvisionProfilePath="$(_EmbeddedProvisionProfilePath)"
			>
